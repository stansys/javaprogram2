public class fractorial
{  public static void main(string [args])
   {
     for(int i=0;i<NUM_FACTS;i++
         system.out.println(it"!is"+ fractorial(i));
    }
    public static int factorial(int n)
           int result=1;
           for(int i=2;i<=n;i++)
                        result*=1;
          return result;
          
 }
 }
          
          
                        
                 
              
